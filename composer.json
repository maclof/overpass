{
    "name": "icecave/overpass",
    "description": "A basic pub/sub and RPC system.",
    "keywords": [
        "pubsub",
        "publish",
        "subscribe",
        "rpc",
        "rmi",
        "remote",
        "procedure",
        "call",
        "amqp",
        "rabbit"
    ],
    "homepage": "https://github.com/IcecaveStudios/overpass",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "require": {
        "php": ">=5.5",
        "eloquent/enumeration": "^5",
        "icecave/isolator": "^2|^3",
        "psr/log": "^1",
        "php-amqplib/php-amqplib": "^2.6"
    },
    "require-dev": {
        "eloquent/asplode": "^2",
        "icecave/archer": "dev-develop",
        "icecave/stump": "^0.3",
        "phake/phake": "^1",
        "phpunit/phpunit": "^4",
        "sami/sami": "^3"
    },
    "autoload": {
        "psr-4": {
            "Icecave\\Overpass\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev"
        }
    }
}

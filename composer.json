    "keywords": [
        "pubsub",
        "publish",
        "subscribe",
        "rpc",
        "rmi",
        "remote",
        "procedure",
        "call",
        "task",
        "queue",
        "job",
        "amqp",
        "rabbit"
    ],
    "homepage": "https://github.com/IcecaveStudios/overpass",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "require": {
        "php": ">=5.5",
        "eloquent/enumeration": "^5",
        "icecave/isolator": "^2|^3",
        "icecave/repr": "^1",
        "php-amqplib/php-amqplib": "^2.6",
        "psr/log": "^1"
    },
    "require-dev": {
        "eloquent/asplode": "^2",
        "icecave/archer": "dev-develop",
        "icecave/stump": "^0.3",
